<p id="notice"><%= notice %></p>

<h1>Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Name</th>
      <th>Track</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= session.start_time %></td>
        <td><%= session.end_time %></td>
        <td><%= session.name %></td>
        <td><%= session.track.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>